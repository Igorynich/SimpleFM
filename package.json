{
  "name": "simple-fm",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "ng build --prod=true --base-href ./ --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:build": "electron-builder build --windows",
    "electron:windows": "npm-run-all electron:serve-tsc build electron:build",
    "robotjs:rebuild": "npm rebuild --runtime=electron --target=7.1.7 --disturl=https://atom.io/download/atom-shell --abi=75",
    "bundle-report": "webpack-bundle-analyzer dist/SimpleFM/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.3",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~9.1.3",
    "@angular/compiler": "~9.1.3",
    "@angular/core": "~9.1.3",
    "@angular/fire": "^6.0.0-rc.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.1.3",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.1.3",
    "@angular/platform-browser-dynamic": "~9.1.3",
    "@angular/router": "~9.1.3",
    "@ng-select/ng-select": "^3.7.1",
    "bootstrap": "^4.4.1",
    "electron-reload": "^1.5.0",
    "firebase": "^7.6.2",
    "hammerjs": "^2.0.8",
    "ngx-auth-firebaseui": "^4.2.1",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.6",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "~9.1.3",
    "@angular/language-service": "~9.1.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "electron": "^7.1.11",
    "electron-builder": "^22.3.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "wait-on": "^4.0.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "@angular-devkit/architect": "~0.900",
    "firebase-tools": "^7.12.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1"
  }
}
