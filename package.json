{
  "name": "simple-fm",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "ng build --prod=true --base-href ./ --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:build": "electron-builder build --windows",
    "electron:windows": "npm-run-all electron:serve-tsc build electron:build",
    "robotjs:rebuild": "npm rebuild --runtime=electron --target=7.1.7 --disturl=https://atom.io/download/atom-shell --abi=75",
    "bundle-report": "webpack-bundle-analyzer dist/SimpleFM/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/fire": "^5.3.0",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.2.11",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "@ng-select/ng-select": "^3.7.1",
    "bootstrap": "^4.4.1",
    "electron-reload": "^1.5.0",
    "firebase": "^7.6.2",
    "hammerjs": "^2.0.8",
    "ngx-auth-firebaseui": "^3.5.0",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.0-rc.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.26",
    "@angular/cli": "~8.3.26",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "electron": "^7.1.11",
    "electron-builder": "^22.3.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "wait-on": "^4.0.0",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
