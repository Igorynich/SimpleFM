{
  "name": "simple-fm",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve -o",
    "ng:serve-en": "ng serve -o --configuration=en",
    "ng:serve:electron": "ng serve -c=electron",
    "i18n-extract": "ng extract-i18n --output-path src/app/locale && xliffmerge --profile xliffmerge.json en",
    "electron:start": "npm-run-all -p ng:serve:electron electron:serve",
    "build": "ng build --prod=true --base-href ./ --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:build": "electron-builder build --windows",
    "electron:windows": "npm-run-all electron:serve-tsc build electron:build",
    "robotjs:rebuild": "npm rebuild --runtime=electron --target=7.1.7 --disturl=https://atom.io/download/atom-shell --abi=75",
    "bundle-report": "webpack-bundle-analyzer dist/SimpleFM/stats-es2015.json",
    "http-serv": "http-server -p 8080 dist/SimpleFM"
  },
  "private": true,
  "dependencies": {
    "@angular-material-extensions/password-strength": "^6.0.0",
    "@angular/animations": "~11.2.6",
    "@angular/cdk": "~11.2.5",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~11.2.6",
    "@angular/localize": "^11.2.6",
    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@angular/service-worker": "~11.2.6",
    "@fullcalendar/core": "^4.4.2",
    "@fullcalendar/daygrid": "^4.4.2",
    "@ng-select/ng-select": "^6.1",
    "@ngrx/effects": "^9.2.1",
    "@ngrx/store": "^9.2.1",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.4",
    "electron-reload": "^1.5.0",
    "firebase": "^8.3.1",
    "immer": "^8.0.4",
    "lodash": "^4.17.21",
    "ngx-auth-firebaseui": "^5.1.0",
    "ngx-pagination": "^5.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^11.3.1",
    "quill": "^1.3.7",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1102.5",
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@angular/language-service": "~11.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.35",
    "codelyzer": "^6.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "tailwindcss": "^2.0.4",
    "ts-loader": "^5.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.7",
    "wait-on": "^4.0.2",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
